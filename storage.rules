rules_version = '2';

service firebase.storage {
	match /b/{bucket}/o {
		match /CMSImages/{ImageFilename} {
      allow write: if request.auth.token.admin == true
			allow read;
		}
		match /rides/{ImageFilename} {
      allow write: if request.auth.token.admin == true
			allow read;
		}
		match /legacyRideImages/{ImageFilename} {
			allow read;
		}
	}
}
