rules_version = '2';
service cloud.firestore {
	match /databases/{database}/documents {
		match /{document=**} {
			allow read, write: if false;
		}


		match /Parks/{ParkId} {
			allow read;
		}
		match /Parks/{ParkId}/Rides/{RideId} {
			allow read;
		}

		match /{path=**}/Rides/{RideId} {
			allow read;
		}
	}
}
